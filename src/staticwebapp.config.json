{
    "routes": [
        {
            "route":"/admin*",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/login",
            "redirect": "/.auth/login/github"
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
        }
    ],
    "responseOverrides": {
        "401": {
            "rewrite": "/login",
            "statusCode": 302
        }
    }
}